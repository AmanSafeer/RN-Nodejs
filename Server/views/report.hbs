<!DOCTYPE html>
<html>

<head>
  <mate charest="utf-8" />
</head>

<body>
  <h1>User List</h1>
  <div id="user_list">
    <ul>
      {{#each users}}
      {{#if this.name}}
      <li>Name: {{this.name}}</li>
      {{else}}
      <li>Name: Aman</li>
      {{/if}}
      <li>Age: {{this.age}}</li>
      <br />
      {{/each}}
    </ul>
  </div>
</body>

</html>